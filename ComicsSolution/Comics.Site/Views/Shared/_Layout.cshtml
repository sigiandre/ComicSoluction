<body class="home blog">
    <partial name="_CookieConsentPartial" />
    <!--Wrapper-->
    <div id="wrapper" class="intro-effect-push">
        <header>
            <div class="ts-main-header main-header-style2 headerMenu">
                <div class="main-header">
                    <div class="over-lay"></div>
                    <h1 class="logo">
                        <a href="~/">
                            <img src="~/imagenes/logo.png" alt="TEsto alternativo" />
                        </a>
                    </h1>
                </div>
                <div class="main-menu-2">
                    <div class="container">
                        <div class="ts-mainmenu-center">
                            <div class="ts-mainmenu">
                                <div class="head-socials">
                                    <a href="javascript:void(0)" class="ts-social-togole"><i class="fa fa-share-alt"></i></a>
                                </div>
                                <a href="javascript:void(0)" class="ts-togole-menu visible-sm visible-xs"><i class="fa fa-bars"></i></a>
                                <nav class="main-menu nav-menu">
                                    <ul class="menu-nav list-inline ts-response-simple ts-response-stack ts-effect-slide-right">
                                        <li class="menu-item current_page_ancestor  ">
                                            <a title="Inicio" href="~/">Inicio</a>
                                        </li>
                                        <li class="menu-item"><a href="~/Home/Nosotros">Nosotros</a></li>
                                        <li class="menu-item menu-item-has-children dropdown">
                                            <a href="#" data-toggle="dropdown" class="dropdown-toggle" aria-haspopup="true">Tienda</a>
                                            <ul role="menu" class=" dropdown-menu">
                                                <li class="menu-item"><a title="Tienda Comics" href="~/Tienda/Index">Tienda</a></li>
                                                <li class="menu-item"><a title="Producto Detalle" href="~/Tienda/Detalle">Producto</a></li>
                                                <li class="menu-item"><a title="Mi Cuenta" href="~/Seguridad/MiCuenta">Mi Cuenta</a></li>

                                            </ul>
                                        </li>
                                        <li class="menu-item"><a href="~/Home/Contacto">Contacto</a></li>
                                        <li class="menu-item menu-item-has-children dropdown">
                                            <a href="#" data-toggle="dropdown" class="dropdown-toggle" aria-haspopup="true">Mantenimientos</a>
                                            <ul role="menu" class=" dropdown-menu">
                                                <li class="menu-item"><a title="Mantenimiento de Revistas" asp-controller="RevistasAjax" asp-action="Index">Revistas</a></li>
                                                <li class="menu-item"><a title="Mantenimiento de Usuarios" asp-controller="Usuario" asp-action="Index">Usuarios</a></li>
                                            </ul>
                                        </li>
                                        @*<li class="mini-shoping-cart-wraper menu-item menu-item-type-post_type menu-item-object-page ">
                                                <a class="cart-contents" href="#" title="Ver tu carrito de compras">
                                                    <i class="fa fa-shopping-cart"></i>
                                                    <span class="cart-number-items">0</span>
                                                </a>
                                                <div class="dropdown-menu mini-shoping-cart">
                                                    <div class="widget_shopping_cart_content">
                                                        <ul class="cart_list product_list_widget ">
                                                            <li class="empty">No hay productos en el carrito.</li>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </li>*@
                                    </ul>
                                </nav>
                                <div class="header-search pull-right">
                                    <span class="search-togole"><i class="fa fa-search"></i></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>

        <div id="ts-search-box">
            <div class="search-overlay"></div>
            <div class="ts-search-pos">
                <div class="container">
                    <div class="ts-serch-inner">
                        <div class="section-title text-center">
                            <h3 class="text-uppercase">Deseas buscar algo?</h3>
                            <div class="hr"></div>
                        </div>
                        <form id="searchform" class="ts-form-search" method="GET">
                            <input type="search" name="s" id="s" value="" placeholder="Escribir y presionar Enter..." />
                            <span><button class="search-submit" type="submit"><i class="fa fa-search"></i></button></span>
                        </form>
                        <span class="close-search">Cerrar</span>
                    </div>
                </div>
            </div>
        </div>

        <div class="ts-social-header">
            <div class="social-overlay"></div>
            <div class="ts-social-inner">
                <div class="section-title  text-center">
                    <h3 class="text-uppercase">Follow Me</h3>
                    <div class="hr"></div>
                </div>
                <ul class="list-social">
                    <li><a target="_blank" href="http://facebook.com"><i class="fa fa-facebook"></i></a></li>
                </ul>
                <span class="close-social">Close</span>
            </div>
        </div>

        @*INICINADO EL CONTENIDO CENTRAL*@

        <!-- Main content -->
        @RenderBody()
        <!-- End / Main content -->
        @*INICIANDO EL PIE DE PAGINA*@
